doctype html

html
  meta(charset="utf-8")
  head
    title InfoSigns
    script(src='/scripts/d3/d3.min.js')
    script(src='experimentr.js')
    link(rel="stylesheet" type="text/css" href="css/main.css")
  body
  style
  script.
    var inPID = '#{outPID}'
    var inGO = JSON.parse(!{outGraphOrder})
    experimentr.setIsBW(#{isBW})
    experimentr.setPID(inPID)
    experimentr.setGO(inGO)
    experimentr.sequence([
      'modules/session2Ins',
      'modules/graphTitles',
      'modules/graphListQuestions',
      'modules/graphInteract',
      'modules/graphTheme',
      'modules/sess2_debrief'
    ]).start()
